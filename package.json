{
  "name": "@suryaumapathy2812/mep-theme-project",
  "version": "1.0.0",
  "description": "Theme with Website layout, Admin Dashboard",
  "license": "MIT",
  "author": "suryaumapathy2812",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/suryaumapathy2812/mep-theme-project"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "ng": "nx",
    "clean": "rimraf dist/* && rimraf tmp/*",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "copy:schematics": "cd libs/theme && npm run build && cd ./../../",
    "pack:theme": "ng build --project theme && npm run copy:schematics && cd dist/libs/theme && npm pack",
    "pack": "npm run clean && npm run pack:theme",
    "publish": "cd dist/libs/theme && npm publish"
  },
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
